{  "version": "0.2.0",
  "configurations": [
    {
      "name": "(gdb) Launch",
      "type": "cppdbg",
      "request": "launch",
      // To launch an specific program, use it's name instead of ${workspaceFolderBasename}
      // "program": "${workspaceFolder}/build/vscode/bin/${workspaceFolderBasename}",
      // "program": "${workspaceFolder}/build/vscode/bin/fit",
      "program": "${workspaceFolder}/build/vscode/bin/tpd",
      // Optional arguments here:
      "args": [],
      "stopAtEntry": true,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ],
      "preLaunchTask": "fpm: install"
    },
    {
